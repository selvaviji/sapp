    <script>
   
        var custid;
        var currentposition;
        var endposition;
        var lastposition;
        var nextbuttonstatus;
        var sms_status;
        var video_status;
        var session_id = "<%= sess_id %>";
        var ipconfig = {siteid:null,sitename:null,ip_address:null};

        //console.log("custid"+custid);
        $(document).ready(function(){
            $("#search").hide();
            $(".searchbtn").click(function(){
                $("#search").toggle();
            });
        });

        refreshing();
        function refreshing(){   
            $.ajax({
                url: '/active/getcustInfo',
                type:'post',
                cache: false,
                success: function (data1){
                    var data=[];
                    var arr1=data1;
                    var options = '<option value="">--ALL--</option>';
                     for( var i=0;i<arr1.custdata.length;i++) {
                        if(custid == arr1.custdata[i].customer_id){
                            options += '<option value="' + arr1.custdata[i].customer_id + '" selected>' + arr1.custdata[i].customer_name + '</option>';    
                        }else{    
                            options += '<option value="' + arr1.custdata[i].customer_id + '">' + arr1.custdata[i].customer_name + '</option>';
                        }
                    }
                    $('#customer_id').html(options);
                }
            });    
            tabledata();
        }

        function tabledata(){    
            var customerid=document.getElementById("customer_id").value;
            var nextval=document.getElementById("nextval").value;
            var rowsperpageval = document.getElementById("rowsperpageval").value;
            var statusvalue = document.getElementById("statusvalue").value;

            console.log("tabledata triggered");
            $.ajax({
                beforeSend:function(){
                   $('#ajaxBusy').show();
                },    

                url: '/active/getdata',
                type: 'POST',
                cache: false, 
                data:{custid:customerid,rowsperpageval:rowsperpageval,statusvalue:statusvalue,nextval:nextval},
                success: function(data){
                    //console.log("Success Triggered");
                    render(data);     
                },error: function(jqXHR, textStatus, err){
                    //console.log("Error Triggered");
                    //alert('text status '+textStatus+', err '+err);
                     $('#ajaxBusy').hide();
                },complete: function(jqXHR, textStatus, err){
                    //console.log("completed Triggered");
                    $('#ajaxBusy').hide();
                    setTimeout(tabledata, 10000);
                }    
            });
        }
     
        function render(data){
            currentposition=data.currentposition;
            endposition=data.endposition;
            lastposition=data.lastposition;
            nextbuttonstatus=data.nextbuttonstatus;
            sms_status=data.sms;
            video_status=data.video;
            //console.log("nextpagevalue......."+data.nextpagevalue);
            document.getElementById("nextval").value=data.nextpagevalue;
            //console.log("currentposition"+currentposition+"endposition"+endposition+"nextbuttonstatus"+nextbuttonstatus);
            //console.log("sms_status"+sms_status);
            //console.log("video_status"+video_status);
            var html  = '<table class="uk-table uk-table-striped uk-table-hover">'+
                                '<thead><tr>'+
                                '<th><strong>Ack</strong></th>'+   
                                '<th><strong>Open Time</strong></th>';
            if(session_id=="1" || session_id=="2"){
                html += '<th><strong>Customer</strong></th>';
            }    
            html += '<th><strong>Site ID</strong></th>'+
                    '<th><strong>Site Name</strong></th>'+
                    '<th><strong>Location</strong></th>'+
                    '<th><strong>Alarm</strong></th>';
            if(sms_status=="true"){
                html+='<th><strong>SMS</strong></th>';
            }
            if(video_status=="true"){
                html+='<th><strong>Video</strong></th>';
            }
            html+='</tr></thead><tbody>';
            var newcontent = '';
            if(data.datas.length>0){
                for (var i = 0; i < data.datas.length; i++){
                    var siteID = data.datas[i].siteid;
                    var siteclass = "siteid_"+data.datas[i].siteid;
                    var ackid = "ackby_"+data.datas[i].id;
                    var site_address = data.datas[i].site_address;
                    var split_data = site_address.split(",");
                    var location="";
                    if(split_data.length>1){
                        location = split_data[0]+",<br/>"+split_data[split_data.length-1];
                    }else{
                        location = site_address;
                    }
                    //console.log("Site ID:"+siteID);
                    // We store html as a var then add to DOM after for efficiency
                    newcontent += "<tr onclick='tblclick("+'"'+data.datas[i].Siteid+'",'+'"'+data.datas[i].site_name+'",'+' "'+(data.datas[i].IP).replace("::ffff:","")+'"'+");'>"+
                                "<td onclick='test("+'"'+data.datas[i].Siteid+'",'+'"'+data.datas[i].site_name+'",'+' "'+data.datas[i].id+'",'+'"'+data.datas[i].user_name+'",'+'"'+data.datas[i].alarmpin_tt+'",'+'"'+data.datas[i].ackflag+'",'+'"'+data.datas[i].use_videolink+'",'+'"'+data.datas[i].site_videolink+'"'+");' id='"+ackid+"'>";
                              if(data.datas[i].ackflag=='1'){
                                newcontent +=  "<i class='uk-icon-thumbs-up green' data-uk-tooltip='{pos:'bottom'}' title="+data.datas[i].user_name+">";
                              }else{
                                //newcontent += data[i].ackflag;
                              }
                              newcontent +="</td>"+ 
                             "<td class='password'>"+moment(data.datas[i].Opentime).format('DD-MM-YYYY H:mm:ss')+"</td>";
                              if(session_id=="1" || session_id=="2"){  
                                newcontent += "<td class='customername'>"+data.datas[i].customer_name+"</td>";
                              }  
                              newcontent += "<td class='siteid'>"+data.datas[i].Siteid+"</td>"+
                              "<td class='sitename'>"+data.datas[i].site_name+"</td>"+
                              "<td class='siteadd'>"+location+"</td>"+
                              "<td class='username'>"+data.datas[i].alarmpin_name+"</td>";
                              if(sms_status=="true" ){
                               newcontent+="<td><a  onclick='showmodal("+'"'+data.datas[i].Siteid+'",'+'"'+data.datas[i].site_name+'",'+' "'+data.datas[i].id+'",'+'"'+data.datas[i].user_name+'",'+'"'+data.datas[i].alarmpin_tt+'",'+'"'+data.datas[i].ackflag+'",'+'"'+data.datas[i].site_videolink+'"'+");' ><i class='uk-icon-envelope' style='color:#1F98BE; text-align:center; width:100%;'></i></a></td>";
                              }
                              
                              if(video_status=="true")
                              {
                               newcontent+="<td><a onclick='checkImageUrl("+'"'+data.datas[i].Siteid+'",'+'"'+data.datas[i].site_name+'",'+'"'+data.datas[i].site_videolink+'"'+");'><i class='uk-icon-video-camera white'   style='color:#1F98BE; text-align:center; width:100%;'></i> </a></td>";
                              }
                                newcontent+="</tr>";
                }
            }
            html += newcontent+'</tbody></table>';
            document.getElementById("alarm-tbl").innerHTML = html;
            pagination();
        }

        tblclick = function (siteid,sitename,ip_address){
            var alerts = '<div>' + siteid + "," + sitename + "," + ip_address +'</div>';  
            ipconfig.siteid=siteid;
            ipconfig.sitename=sitename;
            ipconfig.ip_address=ip_address;
            $('#sitevalue').html(alerts);
            console.log("TR Trigger"+siteid);
        } 

        confirm = function (){
            document.getElementById("cfm1").style.display="block";
            console.log("site:"+ipconfig.siteid);
        }

        yes = function (){
            document.getElementById("cfm2").style.display="block";
            document.getElementById("cfm1").style.display="none";
            $.ajax({
                    url: '/common/hooter',
                    type:'post',
                    cache: false,
                    data: {site_id:ipconfig.siteid,hooter_status:"1;1",ip_address:ipconfig.ip_address},
                    success: function (data){
                        console.log("success:"+data);
                    }

            });
        }

        nos = function(){
            document.getElementById("cfm1").style.display="none";
        }

        stop = function (siteid,ip_address){
            document.getElementById("cfm2").style.display="none";
            $.ajax({
                url: '/common/hooter',
                type:'post',
                cache: false,
                data: {site_id:ipconfig.siteid,hooter_status:"1;0",ip_address:ipconfig.ip_address},
                success: function (data){
                    console.log("success :"+data);
                }
            });
        }


        pagination = function (){
            
            document.getElementById("navigation").innerHTML="";
            var rownumber=document.getElementById("rowsperpageval").value;
            var rowsShown = rownumber;
            rowsShown=parseInt(rowsShown);
            numberofrecords=100*(rowsShown/10);
            //console.log("rowsShown"+rowsShown);
            currentposition=parseInt(currentposition);
            var rowsTotal = $('#alarm-tbl tbody tr').length;
            //console.log("datas are:::"+rowsTotal)
            if(((currentposition*rowsShown)>numberofrecords) ){
                    $('#navigation').append('<li><a onclick="previousrecords()" style="display:block" id="previousid" ><span>Prev</span></a></li>');
                }
                if((currentposition*rowsShown)<=numberofrecords){
                    $('#navigation').append('<li ><a onclick="previousrecords()" style="display:none" id="previousid" ><span>Prev</span></a></li>');
                }
             if(endposition>1){
                for(currentposition;currentposition<=endposition;currentposition++) {
                        //console.log("appending");
                        var pagenum=currentposition-1;
                        $('#navigation').append('<li><a href="#" rel="'+pagenum+'">'+currentposition+'</a><li> ');
                    }

             }
             
             if((endposition<lastposition)|| nextbuttonstatus==1)
