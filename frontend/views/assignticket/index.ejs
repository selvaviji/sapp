<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../../partials/head %>
    <script>
        var datas=<%-JSON.stringify(datas)%>;
        var fields=<%-JSON.stringify(fielddata)%>;
        var arr=datas;
        var arr1=fields;
        $(document).ready(function(){
            $("#search").hide();

            $(".searchbtn").click(function(){
                $("#search").toggle();
            });
        });
    </script>
</head>

<body class="sidebar_main_open sidebar_main_swipe">
    <!--Top Header-->
    <% include ../../partials/mainheader %>
    <!--Top Header-->
    <% include ../../partials/sidemenu %>

    <!--Start Content-->
    <div id="page_content">
        <div id="page_content_inner">
            <div class="searchbtn"><i class="uk-icon-search"></i></div>
            <div class="dwnbtn"><i class="uk-icon-download padright"></i></div>
            <!--Dashboard-->
            <h3 class="heading_b uk-margin-bottom">All Tickets</h3>
            <!--End Dashboard-->
            <div class="md-card" id="search">
                <div class="md-card-content">
                    <div class="uk-grid uk-dropdown-grid">
                        <div class="uk-form-row valid" >
                            <select id="fieldsupervisor"  name="dateformat" class="md-input">
                            </select>
                        </div>
                        
                        <div class="uk-width-1-4" style="margin-top: 20px;">
                            <button type="submit" id="s1" class="md-btn md-btn-success" onclick="change()">
                            <i class="uk-icon-search white padright"></i>Aplly</button>
                        </div>
                    </div>  
                </div>
            </div>
            
            <div class="md-card ">
                <div class="md-card-content">
                    <div class="uk-overflow-container " id="tabledata">
                        
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!--End Content--> 

<!--Start Script-->
<script src="/moment/min/moment.min.js"></script>
<script src="/js/common.min.js"></script>
<script src="/js/uikit_custom.min.js"></script>
<script src="/js/altair_admin_common.min.js"></script>
<script src="/js/alarmtickets.js"></script>
<script>
ticketdisplay(arr,arr1);
</script>
<script>
$(function() {

    var data=[];
          
    for( var i=0;i<arr1.length;i++)
    {
        var obj={"id":arr1[i].user_name,"name":arr1[i].user_name};
        data.push(obj);
                
            }
    $.each(data, function(i, option) {
        $('#fieldsupervisor').append($('<option/>').attr("value", option.id).text(option.name));
    });
})
       
</script>

</body>
</html>