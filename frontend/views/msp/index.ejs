<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../../partials/head %>
</head>

<body class="sidebar_main_open sidebar_main_swipe">
    <% include ../../partials/mainheader %>
    <% include ../../partials/sidemenu %>
    <!--Start Content-->
    <div id="page_content">
        <div id="page_content_inner">
        <h3 class="heading_b uk-margin-bottom">MSP List</h3>
        <!--Content Page-->
        <div class="md-card">
            <div class="md-card-content">
                <div class="uk-grid" data-uk-grid-margin>
                    <div class="uk-width-medium-1-1">
                        <table class="display" id="example" width="100%">
                            <thead>
                                <tr>
                                    <th><strong><input type="checkbox" id="chkall" onclick="setCheckboxValues(this);"></strong></th>
                                    <th><strong>Customer Name</strong></th>
                                    <th><strong>MSP Name</strong></th>
                                    <th><strong>Contact Person</strong></th>
                                    <th><strong>Mobile No</strong></th>
                                    <th><strong>Email</strong></th>
                                    <th><strong>Edit</strong></th>
                                    <th><strong>Delete</strong></th>
                                </tr>
                            </thead>
                            <%  if(JSON.stringify(datas) === '{}'){
                                    console.log("step-2");
                                }else{
                                    datas.forEach(function(d) { %>
                                        <tr>
                                            <td>
                                              <input type="checkbox" name="chkuser[]" value="<%= d.msp_id%>">
                                            </td>
                                            <td><%= d.customer_name %></td>
                                            <td><%= d.msp_name %></td>
                                            <td><%= d.msp_contact_name %></td>
                                            <td><%= d.msp_contact_no %></td>
                                            <td><%= d.msp_contact_email %></td>
                                            <td><a href='/msp/edit/<%=d.msp_id%>' class='uk-icon-edit editicon' title='Edit'></td>
                                            <td><a  href='/msp/delete/<%=d.msp_id%>' onclick='return confirm("Are you sure to delete?")' class='uk-icon-trash editicon'  title='Delete' ></td>
                                        </tr>
                                    <% });  
                                }%>
                           
                        </table>
                    </div>
                </div>
            </div>
        </div>   
        <!--End of Content Page-->
    </div>
</div>

<!--End Content--> 


<!--Start Script-->
<% include ../../partials/footer %> 

<script type="text/javascript" charset="utf-8">
   $(document).ready(function() {
    $('#example').DataTable();
    $('<div class="md-btn md-btn-success "><a href="/msp/addmsp" ><i class="uk-icon-user white paddingleft"></i>Create MSP</a></div>').appendTo('div.btn1');
    $('#example')
    .removeClass( 'display' )
    .addClass('uk-table uk-table-striped');
  });

  // For demo to fit into DataTables site builder...
  
</script>
    
</body>
</html>