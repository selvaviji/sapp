<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../../partials/head %>
    <link rel="stylesheet" type="text/css" href="/css/datatables.css"/>
</head>

<body class="sidebar_main_open sidebar_main_swipe">
 
     <!--Top Header-->
    <% include ../../partials/mainheader %>
    <!--Top Header-->
    <% include ../../partials/sidemenu %>
  
    <!--Content-->
    <!--Start Content-->

    <div id="page_content">
        <div id="page_content_inner">
        <h3 class="heading_b uk-margin-bottom">Role List</h3>
        <!--Button-->
        <!-- <div class="md-card">
            <div class="md-card-content">
                <div class="uk-grid">
                    <div class="uk-width-medium-3-4">
                        <div class="md-btn md-btn-success ">
                            <a href="/userrole/addrole" ><i class="uk-icon-user white paddingleft"></i>Create Role</a>
                        </div>
                    </div>
                   
                </div> 
            </div>
        </div> -->
        <!--End Button-->

        <!--Content Page-->
        <div class="md-card ">
            <div class="md-card-content">
                <div class="">
                    <div class="uk-width-medium-1-1" id="customers">
                        <table class="display" id="example" width="100%">
                           <thead>
                                    <tr>
                                        <th><strong>Role Name</strong></th>
                                        <th><strong>Role Desc </strong></th>
                                        <th><strong>Created Date</strong></th>
                                        <th><strong>Updated Date</strong></th>
                                        <th><strong>Edit</strong></th>
                                        <th><strong>Delete</strong></th>
                                            
                                    </tr>
                           </thead>
                            <tbody>
                            <%  if(JSON.stringify(datas) === '{}'){
                                    console.log("step-2");
                                }else{
                                    datas.forEach(function(d) { %>
                                        <tr>
                                            
                                            <td><%= d.role_name%></td>
                                            <td><%= d.role_desc%></td>
                                            <td><%=moment(d.created_date).format('DD-MM-YYYY H:mm:ss')%></td>
                                            <td><%=moment(d.updated_date).format('DD-MM-YYYY H:mm:ss')%></td>
                                            <td><a href='/userrole/edit/<%=d.role_id%>' class='uk-icon-edit editicon' title='Edit'></td>
                                            <td><a  href='/userrole/delete/<%=d.role_id%>' onclick='return confirm("Are you sure to delete?")' class='uk-icon-trash editicon'  title='Delete' ></td>
                                        </tr>
                           <%       });  
                                } %> 
                    
                        </tbody> 
                        </table>
                         
                    </div>
                </div>
            </div>
        </div>   
        <!--End of Content Page-->
    </div>
</div>
<!--End Content--> 
<!--Start Script-->

<% include ../../partials/footer %>

<script type="text/javascript" src="/js/datatables.js"></script>
<script type="text/javascript" charset="utf-8">
   $(document).ready(function() {
    $('#example').DataTable();
     //$('<button id="refresh">Refresh</button>').appendTo('div.uk-width-1-4');
     $('<div class="md-btn md-btn-success "><a href="/userrole/addrole" ><i class="uk-icon-user white paddingleft"></i>Create Role</a></div>').appendTo('div.uk-width-1-6');
  } );
</script>
<script type="text/javascript">
  // For demo to fit into DataTables site builder...
  $('#example')
    .removeClass( 'display' )
    .addClass('uk-table uk-table-striped');
</script>

</body>
</html>