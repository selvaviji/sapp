<html lang="en">
<head>
    <% include ../partials/head %>
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
</head>

<body class="sidebar_main_open sidebar_main_swipe">
    <% include ../partials/mainheader %>
    <!--Top Header-->
    <% include ../partials/sidemenu %>
    
<!--Start Content-->
<div id="page_content">
<div id="page_content_inner">
    <% if(sess_id==1){%>
    <div class="uk-grid">
        <div class="uk-width-medium-1-1" style="display:block; float:right; text-align:right; margin-bottom:5px;">
            CUSTOMER :
            <select id="customer_id" name="customer_id">
            </select>
        </div>
        </div>
    <%}%>
<div class="uk-grid uk-grid-width-large-1-4 uk-grid-width-medium-1-2 uk-grid-medium uk-sortable sortable-handler hierarchical_show" data-uk-sortable data-uk-grid-margin>
    <div>
        <a href="/active">    
            <div class="md-card dash-3">
                <div class="md-card-content">
                    <div class="uk-margin-top"><i class="uk-icon-bell dash-icon"></i></div>
                    <span class="uk-text-large" style="color:#fff;">Fees</span>
                    <h2 class="uk-margin-remove">
                        <span class="countUpMe"  id="activealarmcount" style="color:#fff;"></span>
                    </h2>
                </div>
            </div>
        </a>  
    </div>
    <div>
        <a href="/alarmlist">
            <div class="md-card dash-4">
                <div class="md-card-content">
                    <div class="uk-margin-top"><i class="uk-icon-bell-o dash-icon"></i></div>
                    <span class="uk-text-large" style="color:#fff;">Students</span>
                    <h2 class="uk-margin-remove" id="peity_live_text"  style="color:#fff;">
                        <span class="countUpMe" id="closedalarmcount" style="color:#fff;"></span>
                    </h2>
                </div>
            </div>
        </a>   
    </div>

    <div>
        <div class="md-card dash-7">
            <div class="md-card-content">
                <div class="uk-margin-top"><i class="uk-icon-ticket dash-icon"></i></div>
                    <span class="uk-text-large" style="color:#fff;">Open Feedback</span>
                    <h2 class="uk-margin-remove" id="peity_live_text"  style="color:#fff;">
                        <span class="countUpMe"  id="ticket_opened_count" style="color:#fff;"></span>
                    </h2>
            </div>
        </div>
    </div>
    <div>
        <div class="md-card dash-8">
            <div class="md-card-content">
                <div class="uk-margin-top">
                    <i class="uk-icon-ticket dash-icon"></i>
                </div>
                <span class="uk-text-large" style="color:#fff;">Closed Feedback</span>
                <h2 class="uk-margin-remove" id="peity_live_text"  style="color:#fff;">
                    <span class="countUpMe"  id="ticket_closed_count" style="color:#fff;"></span>
                </h2>
            </div>
        </div>
    </div>

    
    
</div>

<div class="uk-grid uk-grid-width-large-1-4 uk-grid-width-medium-1-2 uk-grid-medium uk-sortable sortable-handler hierarchical_show" data-uk-sortable data-uk-grid-margin>
    <div>
    
    <div>
        <div class="md-card dash-5">
            <div class="md-card-content">
                <div class="uk-margin-top"><i class="uk-icon-globe dash-icon"></i></div>
                <span class="uk-text-large" style="color:#fff;">Total Students</span>
                <h2 class="uk-margin-remove" id="peity_live_text"  style="color:#fff;">
                    <span class="countUpMe"  id="sitescount" style="color:#fff;"></span>
                </h2>
            </div>
        </div>
    </div>
</div>
</div> 
</div>
    <!--End Content--> 


<script type="text/javascript">
   $.ajax({
        url: '/active/getcustInfo',
        type:'post',
        cache: false,
        success: function (data1){
            var data=[];
            var arr1=data1;
            var options = '<option value="">--ALL--</option>';
            for( var i=0;i<arr1.custdata.length;i++) {
                options += '<option value="' + arr1.custdata[i].customer_id + '">' + arr1.custdata[i].customer_name + '</option>';
            }
            $('#customer_id').html(options);
        }
    });
</script>

<!--Start Script-->
<script src="/moment/min/moment.min.js"></script>
<script src="/js/common.min.js"></script>
<script src="/js/uikit_custom.min.js"></script>
<script src="/js/altair_admin_common.min.js"></script>

</body>
</html>
